<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="410.0" prefWidth="600.0" stylesheets="@../../Estilos.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.codedynamic.clinica.vista.paciente.antecedente.AntecedenteGinecControlador">
   <children>
      <Label layoutX="147.0" layoutY="2.0" text="Antecedentes Gineco-Obstetricos">
         <font>
            <Font name="Segoe UI Bold" size="20.0" />
         </font>
      </Label>
      <VBox layoutX="14.0" layoutY="51.0" spacing="30.0">
         <children>
            <HBox spacing="53.0">
               <children>
                  <JFXTextField fx:id="edadMenarcaTextField" alignment="CENTER" prefHeight="30.0" promptText="Edad Menarca" />
                  <JFXTextField fx:id="edadClimaterioTextField" alignment="CENTER" prefHeight="30.0" promptText="Edad Climaterio" />
                  <JFXTextField fx:id="menopausiaTextField" alignment="CENTER" prefHeight="30.0" promptText="Menopausia" />
               </children>
            </HBox>
            <HBox spacing="53.0">
               <children>
                  <JFXTextField fx:id="fumTextField" alignment="CENTER" prefHeight="30.0" promptText="FUM" />
                  <JFXTextField fx:id="terapiaAnticonceptivaTextField" alignment="CENTER" prefHeight="30.0" prefWidth="156.0" promptText="Terapia Anticonceptiva" />
                  <JFXTextField fx:id="inicioSexualTextField" alignment="CENTER" prefHeight="30.0" prefWidth="156.0" promptText="Inicio de vida sexual activa">
                     <font>
                        <Font size="11.0" />
                     </font>
                  </JFXTextField>
               </children>
            </HBox>
            <HBox spacing="20.0">
               <children>
                  <JFXTextField fx:id="nroParejaTextField" alignment="CENTER" prefHeight="30.0" prefWidth="130.0" promptText="Numero de Parejas" />
                  <JFXTextField fx:id="cirugiaPelvaniaTextField" alignment="CENTER" prefHeight="30.0" prefWidth="130.0" promptText="Cirugia Pelviana" />
                  <JFXTextField fx:id="papTextField" alignment="CENTER" prefHeight="30.0" prefWidth="130.0" promptText="PAP" />
                  <JFXTextField fx:id="fechaTextField" alignment="CENTER" prefHeight="30.0" prefWidth="130.0" promptText="Fecha" />
               </children>
            </HBox>
            <JFXTextField fx:id="mamografiaTextField" prefHeight="40.0" prefWidth="550.0" promptText="Mamografia o Ecografia Mamaria" />
            <JFXTextField fx:id="estJfxTextField" prefHeight="40.0" prefWidth="550.0" promptText="EST" />
         </children>
      </VBox>
      <HBox layoutX="270.0" layoutY="353.0" spacing="20.0">
         <children>
            <JFXButton onAction="#aceptar" prefHeight="45.0" prefWidth="92.0" styleClass="btnsjfx" text="ACEPTAR" />
            <JFXButton onAction="#modificar" prefHeight="45.0" prefWidth="92.0" styleClass="btnsjfx" text="MODIFICAR" />
            <JFXButton onAction="#salir" prefHeight="45.0" prefWidth="92.0" styleClass="btnsjfx" text="SALIR" />
         </children>
      </HBox>
   </children>
</AnchorPane>
