<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="250.0" prefWidth="700.0" stylesheets="@../Estilos.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.codedynamic.clinica.vista.tratamientos.TratamientoRegistroControlador">
   <children>
      <HBox layoutX="25.0" layoutY="41.0" spacing="50.0">
         <children>
            <JFXTextField fx:id="nombreField" alignment="CENTER" prefHeight="35.0" prefWidth="300.0" promptText="NOMBRE" />
            <JFXTextField fx:id="precioField" alignment="CENTER" prefHeight="35.0" prefWidth="300.0" promptText="PRECIO" />
         </children>
      </HBox>
      <JFXTextField fx:id="descripcionField" alignment="CENTER" layoutX="25.0" layoutY="122.0" prefHeight="40.0" prefWidth="650.0" promptText="DESCRIPCION" />
      <HBox layoutX="216.0" layoutY="186.0" spacing="50.0">
         <children>
            <JFXButton onAction="#aceptar" prefHeight="50.0" prefWidth="120.0" styleClass="btnsjfx" text="GUARDAR" />
            <JFXButton onAction="#cancelar" prefHeight="50.0" prefWidth="120.0" styleClass="btnsjfx" text="CANCELAR" />
         </children>
      </HBox>
   </children>
</AnchorPane>
